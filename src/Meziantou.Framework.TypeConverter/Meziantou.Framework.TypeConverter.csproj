<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.1;netstandard2.0;net461</TargetFrameworks>
    <Description>Convert object to an other type</Description>
    <Version>2.0.2</Version>
    <DefineConstants>ReflectionUtilities_Interal;$(DefineConstants)</DefineConstants>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <Compile Include="..\Meziantou.Framework\ReflectionUtilities.cs" Link="ReflectionUtilities.cs" />
  </ItemGroup>

</Project>
